    "def next_smaller(arr):\n",
    "    op=[]\n",
    "    for i in range(len(arr)):\n",
    "        alpha=-1\n",
    "        for j in range(i+1,len(arr)):\n",
    "            if arr[j]<arr[i]:\n",
    "                alpha=arr[j]\n",
    "                break\n",
    "         op.append(alpha)   \n",
    "    \n",
    "arr=[1,2,4,3]\n",
    "res=next_smaller(arr)\n",
    "    "
