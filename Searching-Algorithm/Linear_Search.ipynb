    "def Linear_Search(arr,ele):\n",
    "    idx=-1\n",
    "    for i in range(0,len(arr)):\n",
    "        if arr[i]==ele:\n",
    "            idx=i\n",
    "            break\n",
    "    \n",
    "    return idx\n",
    "arr=[3,4,0,1,6]\n",
    "ele=66\n",
    "res=Linear_Search(arr,ele)\n",
    "print(res)"
   ]
