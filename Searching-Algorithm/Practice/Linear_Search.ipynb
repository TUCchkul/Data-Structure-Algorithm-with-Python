    "def Linear_Search(arr, ele):\n",
    "    ind=-1\n",
    "    for i in range(0, len(arr)):\n",
    "        if ele==arr[i]:\n",
    "            ind=i\n",
    "            break\n",
    "    return ind\n",
    "arr=[0,6,9,3,5,7,10]\n",
    "ele=101\n",
    "ind=Linear_Search(arr, ele)\n",
    "print(ind)"
   ]
