    "def insertion_Sort(arr):\n",
    "    for i in range(len(arr)):\n",
    "        j=i-0\n",
    "        while j>=0 and arr[j]>arr[j+1]:\n",
    "            arr[j],arr[j+1]=arr[j+1],arr[j]\n",
    "            j-=1\n",
    "            print(arr)\n",
    "        print()\n",
    "            \n",
    "arr=[5,4,3,2,1]\n",
    "print(arr)\n",
    "insertion_Sort(arr)\n",
    "#print(arr)"
